  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build:tsCheck": "tsc --build tsconfig.json",
    "build:package": "cp package.json ./dist",
    "build:scripts": "cp -r ./scripts ./dist",
    "build:nginx": "cp -r ./.platform ./dist",
    "build:files": "babel ./ --extensions .ts,.js -d dist --ignore **/__mocks__,**/node_modules,**/*.test.js",
    "build": "rm -rf ./dist && npm run build:tsCheck && npm run build:files && npm run build:package && npm run build:scripts && npm run build:nginx",
    "build:app": "cd ./dist && zip ../app.zip -r .",
    "start:nodemon": "nodemon index.ts",
    "start:debug": "node --inspect -r ts-node/register -r tsconfig-paths/register index.ts",
    "start:redis": "docker compose -f ./docker-compose-redis.yml up",
    "start:postgres": "docker compose -f ./docker-compose-db.yml up",
    "start:app": "docker compose -f ./docker-compose-app.yml up",
    "start": "node index.js"
  },
